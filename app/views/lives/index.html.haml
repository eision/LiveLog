- provide(:title, 'Live List')
%header.page-header
  %h1 Live List

- Live.years.each_slice(2) do |two_y|
  .row
    - two_y.each do |y|
      - y_live = @lives.where(date: nendo_range(y))
      - if y_live.exists?
        .col-sm-6
          .panel.panel-default
            .panel-heading
              %h2.panel-title= y
            %table.table.table-hover
              %tbody
                = render y_live
